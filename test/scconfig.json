{
  "env": "testnet",
  "main": "@modules/module1",
  "moduleDir": "modules",
  "modules": {
    "@modules/module1": {
      "path": "module1"
    }
  },
  "dependencies": {
    "@scom/ton-core": "*"
  },
  "menus": [
    {
      "caption": "Module 1",
